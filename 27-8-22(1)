#include <stdio.h>
#include <string.h>

int main()
{
    char s1[20],s2[20];
    int i,j,temp1,temp2,n1,n2;
    printf("enter string:");
    gets(s1);
    n1=strlen(s1);
    for(i=0;i<n1-1;i++)
    {
        for(j=0;j<n1-i-1;j++)
        {
            if(s1[j]>s1[j+1])
            {
                temp1=s1[j];
                s1[j]=s1[j+1];
                s1[j+1]=temp1;
            }
        }
    }
    puts(s1);
    printf("enter string:");
    gets(s2);
    n2=strlen(s2);
    int k,l;
    for(k=0;k<n2-1;k++)
    {
        for(l=0;l<n2-k-1;l++)
        {
            if(s2[l]>s2[l+1])
            {
              temp2=s2[l];
              s2[l]=s2[l+1];
              s2[l+1]=temp2;
            }
        }
    }
    puts(s2);
    int count=0;
    for(i=0;s1[i]!='\0';i++)
    {
        if(s1[i]==s2[i])
        count++;
    }
    if(count==n1)
    {
        printf("anagram");
    }
    else
    {
        printf("not anargam");
    }
    return 0;
}
