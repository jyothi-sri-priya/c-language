#include <stdio.h>

int main()
{
    int n,a,r,sum=0;
    printf("enter number:");
    scanf("%d",&n);
    a=n;
    while(n>0)
    {
        r=n%10;
        sum=sum+r;
        n=n/10;
        
    }
    printf("sum:%d\n",sum);
    if(a % sum==0)
    {
        printf("Harshad Number");
    }
    else
    {
        printf(" Not Harshad number");
    }

    return 0;
}
