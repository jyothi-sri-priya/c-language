#include <stdio.h>

int main()
{
    int n,i,a[50],m,fact,sum=0;
    printf("enter n");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++) 
    {
        fact=1;
        m=a[i];
    while(m>0)
    {
        fact=fact*m;
        m--;
    }
    sum=sum+fact;
}
   printf("%d",sum);
    return 0;
}
