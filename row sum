#include <stdio.h>

int main()
{
    int a[100][100];
    int i,j,r,c,sum=0;
    printf("how many rows");
    scanf("%d",&r);
    printf("how many columns");
    scanf("%d",&c);
    for(i=0;i<r;i++)
    {
        for(j=0;j<c;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<r;i++)
    {  
       sum=0;
        for(j=0;j<c;j++)
        {
            sum=sum+a[i][j];
        }
    printf("%d row sum:%d\n",i+1,sum);
    }
    return 0;
}
